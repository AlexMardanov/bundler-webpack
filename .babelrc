{
    "presets": [
        [
            "env",
            {
                "targets": {
                    "browsers": [
                        "last 2 versions"
                    ]
                }
            }
        ],
        "react"
    ],
    "plugins": [
        "syntax-dynamic-import",
        "transform-class-properties",
        "react-hot-loader/babel",
        "transform-object-rest-spread",
        [
            "transform-runtime",
            {
                "helpers": false,
                "polyfill": false,
                "regenerator": true,
                "moduleName": "babel-runtime"
            }
        ]
    ]
}